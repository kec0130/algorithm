   "source": [
    "### 24. Swap Nodes in Pairs"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# value swap\n",
    "class Solution:\n",
    "    def swapPairs(self, head: ListNode) -> ListNode:\n",
    "        curr = head\n",
    "        while curr and curr.next:\n",
    "            curr.val, curr.next.val = curr.next.val, curr.val\n",
    "            curr = curr.next.next\n",
    "        return head"
   ]
  }
